  "repository": {
    "type": "git",
    "url": "https://github.com/ReTable/ui-kit.git",
    "directory": "components/ui-theme"
  },
  "author": "Retable Inc.",
  "license": "ISC",
  "files": [
    "lib",
    "sass",
    "typings"
  ],
  "scripts": {
    "build": "forge",
    "build:docs": "forge --storybook",
    "build:watch": "forge --watch",
    "test": "vitest run"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "change-case": "^5.4.4",
